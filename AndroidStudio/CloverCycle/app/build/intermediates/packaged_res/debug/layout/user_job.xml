<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <EditText
        android:id="@+id/amountInput"
        android:layout_width="209dp"
        android:layout_height="67dp"
        android:layout_marginBottom="24dp"
        android:ems="10"
        android:inputType="text"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/jobBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/jobBtn"
        android:layout_width="182dp"
        android:layout_height="53dp"
        android:backgroundTint="#0B2A1A"
        android:text="Post My Job"
        android:textColor="#1CA83C"
        android:textColorHighlight="#0A291A"
        app:layout_constraintBottom_toTopOf="@+id/paymentsBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/menuBtn"
        android:layout_width="182dp"
        android:layout_height="53dp"
        android:backgroundTint="#0B2A1A"
        android:text="Log Out"
        android:textColor="#1CA83C"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/paymentsBtn"
        android:layout_width="182dp"
        android:layout_height="53dp"
        android:backgroundTint="#0B2A1A"
        android:textColor="#1CA83C"
        android:textColorHighlight="#0A291A"
        android:text="My Payments"

        app:layout_constraintBottom_toTopOf="@+id/menuBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="208dp"
        android:layout_height="192dp"
        android:layout_marginBottom="96dp"

        android:contentDescription="profile"
        app:layout_constraintBottom_toTopOf="@+id/nameTxt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/clovercycle" />

    <EditText
        android:id="@+id/nameTxt"
        android:layout_width="207dp"
        android:layout_height="65dp"
        android:layout_marginBottom="72dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/addressTxt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/addressTxt"
        android:layout_width="206dp"
        android:layout_height="65dp"
        android:layout_marginBottom="60dp"
        android:ems="10"
        android:inputType="textPostalAddress"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/amountInput"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/name"
        android:layout_width="73dp"
        android:layout_height="29dp"
        android:text="Name:"
        android:textColor="#0A291A"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.342"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/address"
        android:layout_width="101dp"
        android:layout_height="22dp"
        android:layout_marginBottom="24dp"
        android:text="Address:"
        android:textColor="#0C2C1F"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.538"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/amount"
        android:layout_width="101dp"
        android:layout_height="22dp"
        android:layout_marginBottom="24dp"
        android:text="Amount:"
        android:textColor="#0C2C1F"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.701"
        tools:ignore="DuplicateIds,MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>